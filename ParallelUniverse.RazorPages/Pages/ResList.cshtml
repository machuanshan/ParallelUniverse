@page
@model ParallelUniverse.RazorPages.Pages.ResListModel

<h1>浏览资源</h1>

<table class="table table-striped">
    <tbody>
        @foreach (var v in Model.FileInfos)
        {
            var target = Uri.EscapeUriString(v.Name);

            <tr>
                <td>
                    <a href="@target">@v.Name</a>
                </td>
                <td>
                    <a asp-page="./PubRes" asp-route-path="@v.FullName">发布</a>
                </td>
            </tr>
        }
    </tbody>
</table>